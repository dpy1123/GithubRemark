{
  "manifest_version": 3,
  "name": "GithubRemark",
  "version": "0.5.5",
  "description": "给你的Github好友增加备注名吧～",
  "icons": {
    "128": "images/github_512.png"
  },
  "background": {
    "service_worker": "bg.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*.github.com/*"
  ],
  "action": {
    "default_title": "GithubRemark",
    "default_icon": {
      "19": "images/github.png"
    },
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "devtools_page": "options.html"
}